generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}


model Pet {
  id        Int      @id @default(autoincrement())
  species   String
  name      String
  role      String
  comment   String
  emoji     String?
  createdAt DateTime @default(now())
}
